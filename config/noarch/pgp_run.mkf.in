export BATCH_OPTIONS
export MAKEFLOW_BATCH_QUEUE_TYPE
export MAKEFLOW_MAX_REMOTE_JOBS
pgp_proc.mkf: $PGP_INPUT_DIR
    @BATCH_OPTIONS=$PGP_BATCH_OPTIONS_PREPARE
    @BATCH_LOCAL=$PGP_BATCH_LOCAL_PREPARE
    @PGP_PREPARE@ $PGP_INPUT_DIR $PGP_OUTPUT_DIR
postproc_flag_ok: pgp_proc.mkf
    @BATCH_OPTIONS=$PGP_BATCH_OPTIONS_PROCESS
    @BATCH_LOCAL=$PGP_BATCH_LOCAL_PROCESS
    MAKEFLOW pgp_proc.mkf . $PGP_NESTED_WRAPPER_PROCESS

